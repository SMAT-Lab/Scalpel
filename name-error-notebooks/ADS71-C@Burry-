    "doc  = doc[~doc['bericht tekst'].str.contains(r'http[s]*')]  # TODO secure?\n",
    "doc  = doc[~doc['auteur'].str.contains('grieptweets')]  # TODO to file\n",
    "doc  = doc[~doc['auteur'].str.contains('kleenex_helpt')]\n",
    "# doc.head()"
