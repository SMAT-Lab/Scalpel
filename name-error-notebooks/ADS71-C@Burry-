   "source": [
    "# Defenition of helper function\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "re_clean = re.compile(r'(https?://\\S+|@\\S+)')\n",
    "re_words = re.compile(r'(\\w+-?\\w*)')\n",
    "\n",
    "def clean_text(text: str):\n",
    "    words = []\n",
    "    if text:\n",
    "        text = re_clean.sub(' ', text)\n",
    "        words = re_words.findall(text)\n",
    "    return words"
   ]
  },
